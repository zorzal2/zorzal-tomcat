<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.fontar.data.impl.domain.bean">
    <class name="TipoAdquisicionBean" table="TIPO_ADQUISICION" >
		<id name="id" type="java.lang.Long" column="ID_TIPO_ADQUISICION">
			<generator class="sequence">
			      <param name="sequence">SQ_TIPO_ADQUISICION</param>
		   </generator>		
		</id>		
        <property name="descripcion" column="DE_DESCRIPCION" type="java.lang.String" />
        <property name="codigo" column="CD_TIPO_ADQUISICION" type="java.lang.String" />
    </class>

</hibernate-mapping>