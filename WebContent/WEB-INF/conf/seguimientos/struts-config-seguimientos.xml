	<!-- Action Mapping Definitions -->
	<action-mappings>

		<action path="/SeguimientoInventario"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="SeguimientoFiltersForm" parameter="inventario"
			validate="false">
			<forward name="success" path=".SeguimientoInventarioDef" />
		</action>

	
		<action path="/SeguimientoInventarioFiltrar"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="SeguimientoFiltersForm" parameter="filtrar" validate="true"
			input="/SeguimientoInventario.do">
			<forward name="success" path=".SeguimientoInventarioDef" />
		</action>

		<action path="/SeguimientosProyectoInventario" 
				type="org.springframework.web.struts.DelegatingActionProxy" 
				name="SeguimientoProyectoFiltersForm"
				parameter="verInventario" 
				scope="request">
				
			<forward name="inventario" path=".SeguimientosProyectoDef" />
		</action>

		<action path="/SeguimientoGuardarEdicion"
			type="org.springframework.web.struts.DelegatingActionProxy" 
			name="SeguimientoEditarDynaForm" 
			parameter="guardarEdicion" 
			scope="request" 
			validate="true" 
			input="/SeguimientoEditar.do">
			
			<forward name="success" path="/SeguimientoVisualizar.do" />

		</action>

		<!-- Rendicion de Cuentas ANR -->
		<action path="/RendicionCuentasANRInventario"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="RendicionCuentasFiltersForm" 
			parameter="inventario"
			validate="false">
			
			<forward name="success" path=".RendicionCuentasANRInventarioDef" />
		</action>

		<action path="/RendicionCuentasANRAgregar"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="RendicionCuentasANREditarDynaForm" 
			parameter="cargar"
			scope="request"	
			validate="false">

			<forward name="success" path=".EditarRendicionCuentasANRDef"/>
		</action>	

		<action path="/RendicionCuentasANREditar"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="RendicionCuentasANREditarDynaForm" 
			parameter="cargar"
			scope="request"	
			validate="false">

			<forward name="success" path=".EditarRendicionCuentasANRDef"/>
		</action>	

		<action path="/RendicionCuentasANRResumen"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="RendicionCuentasResumenDynaForm" 
			parameter="resumen"
			scope="request"
			validate="false">

			<forward name="success" path=".RendicionCuentasANRResumenDef"/>
		</action>
			
		<action path="/RendicionCuentasANRGuardar"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="RendicionCuentasANREditarDynaForm" 
			parameter="guardar"
			scope="request"
			validate="true"
			input="/RendicionCuentasANRAgregar.do">

			<forward name="success" path="/RendicionCuentasANRInventario.do"/>
			<forward name="invalid"	path="/RendicionCuentasANRAgregar.do"/>
		</action>
		<action path="/RendicionCuentasANRBorrar"
			type="org.springframework.web.struts.DelegatingActionProxy"
			parameter="guardar"
			scope="request"
			input="/RendicionCuentasANRInventario.do">

			<forward 
				name="success"
				path="/RendicionCuentasANRInventario.do" redirect="true">
			</forward>
		</action>
		<!-- Fin Rendicion de cuentas ANR -->
		<!-- Rendicion de Cuentas CF -->
		<action path="/RendicionCuentasCFInventario"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="RendicionCuentasFiltersForm" 
			parameter="inventario"
			validate="false">
			
			<forward name="success" path=".RendicionCuentasCFInventarioDef" />
		</action>

		<action path="/RendicionCuentasCFAgregar"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="RendicionCuentasCFEditarDynaForm" 
			parameter="cargar"
			scope="request"	
			validate="false">

			<forward name="success" path=".EditarRendicionCuentasCFDef"/>
		</action>	

		<action path="/RendicionCuentasCFEditar"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="RendicionCuentasCFEditarDynaForm" 
			parameter="cargar"
			scope="request"	
			validate="false">

			<forward name="success" path=".EditarRendicionCuentasCFDef"/>
		</action>	

		<action path="/RendicionCuentasCFResumen"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="RendicionCuentasResumenDynaForm" 
			parameter="resumen"
			scope="request"
			validate="false">

			<forward name="success" path=".RendicionCuentasCFResumenDef"/>
		</action>
			
		<action path="/RendicionCuentasCFGuardar"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="RendicionCuentasCFEditarDynaForm" 
			parameter="guardar"
			scope="request"
			validate="true"
			input="/RendicionCuentasCFAgregar.do">

			<forward name="success" path="/RendicionCuentasCFInventario.do"/>
			<forward name="invalid"	path="/RendicionCuentasCFAgregar.do"/>
		</action>

		<action path="/RendicionCuentasCFBorrar"
			type="org.springframework.web.struts.DelegatingActionProxy"
			parameter="guardar"
			scope="request"
			input="/RendicionCuentasCFInventario.do">

			<forward 
				name="success"
				path="/RendicionCuentasCFInventario.do" redirect="true">
			</forward>

		</action>
		<!-- Fin Rendicion de cuentas CF -->

		<action path="/SeguimientoProyectoInventarioFiltrar" 
               type="org.springframework.web.struts.DelegatingActionProxy" 
