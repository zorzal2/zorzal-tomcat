			<forward name="success" 
				path=".EvaluacionSeguimientoInventarioDef"/>
        </action>

		<action path="/EvaluacionSeguimientoInventarioFiltrar" 
			type="org.springframework.web.struts.DelegatingActionProxy" 
			name="EvaluacionSeguimientoFiltersForm" 
