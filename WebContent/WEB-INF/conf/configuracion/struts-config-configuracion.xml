	<!-- === Action Mapping Definitions -->
	<action-mappings>

		<action path="/EnPaquetePaginaError"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="DummyDynaForm" parameter="enPaquete" scope="request"
			validate="false" input="">
			<forward name="success" path=".PaqueteErrorDef"></forward>
		</action>

		<action path="/LocalizacionGuardar"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="LocalizacionEditarDynaForm" parameter="guardar" scope="request"
			validate="true" input="/LocalizacionEditar.do">
			<forward name="success" path=".LocalizacionEditarDef">
			</forward>
		</action>

		<action path="/Entidad*Borrar"
			type="org.springframework.web.struts.DelegatingActionProxy"
			parameter="borrar"
			scope="request" validate="false">
			<forward name="success"
				path="/Entidad{1}ConfiguracionInventarioFiltrar.do">
			</forward>
			<forward name="invalid"
				path="/Entidad{1}ConfiguracionInventarioFiltrar.do">
			</forward>
		</action>

		<action path="/EntidadBeneficiariaEditar"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="EntidadBeneficiariaEditarDynaForm" parameter="editar"
			scope="request" validate="false">
			<forward name="success"
				path=".EntidadBeneficiariaEditarDef">
			</forward>
		</action>

		<action path="/EntidadBeneficiariaGuardar"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="EntidadBeneficiariaEditarDynaForm" parameter="guardar"
			scope="request" validate="true"
			input="/EntidadBeneficiariaEditar.do">
			<forward name="success"
				path=".EntidadBeneficiariaEditarDef">
			</forward>
			<forward name="invalid"
				path="/EntidadBeneficiariaEditar.do">
			</forward>
		</action>

		<action path="/FacturacionData"
			type="org.springframework.web.struts.DelegatingActionProxy"
